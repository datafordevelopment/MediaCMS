<%= feed_title_block('All My Photos', {:controller => 'feed', :action => :latest_member_photos, :id => @photographer.id}, {:auto_rss => true}) %>
<div class="block_title"><h2>Viewing all photos by <%= @photographer.pretty_name %></h2></div>
<small><%= will_paginate @photos  %></small>
<div class="step">
  <% for photo in @photos %>
    <div class="listPhotos">
      <%= render :partial => 'photos/small_picture', :locals => { :photo => photo} %>
    </div>
  <% end %>
</div>
<small><%= will_paginate @photos %></small>  
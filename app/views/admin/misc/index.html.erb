<h2>Misc Adminstration</h2>
<div class="step">
  <table cellpadding="0" cellspacing="0" class="outer">
    <tr>
      <td class="head">Resync user photo counters</td>
      <td class="even">
        <%= form_tag '/admin/misc/sync_users' %>
          <input type="submit" value="Resync Users">
        </form> 
       </td>
    </tr>
    <tr>
      <td class="head">Sync Photos and license prices</td>
      <td class="even">
        <%= form_tag '/admin/misc/sync_licenses' %>
          <input type="submit" value="Resync Licenses">
        </form> 
       </td>
    </tr>
  </table>
</div>
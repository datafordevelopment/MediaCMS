<h2>Photo Delete Confirmation</h2>
<table cellpadding="0" cellspacing="5" id="photo_table">
  <tr><td><h1><%= h @media.title -%></h1></td></tr>
  <tr><td align="center">
    <%= render_view(@media) -%>
  </td></tr>
  <tr><td  align="center">
    <% form_for :media, :url => {:action => :delete_photo, :id => @media.id} do |f| -%>
      <%= f.submit 'Confirm Delete Photo' %>
    <% end -%>
  </td></tr>
</table>